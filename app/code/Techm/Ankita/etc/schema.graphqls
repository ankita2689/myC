
type Query {
    attributeValues (
        sku: String @doc(description: "SKU of the product")
    ): [Productdata] @resolver(class: "Techm\\Ankita\\Model\\Resolver\\Products") @doc(description: ""Query by fetch custom attribute value")
}

type Productdata @doc(description: "Custom Attributes to show in Product Details Page") {
    prod_type : String  @doc(description: "prod_type atribute")
}
